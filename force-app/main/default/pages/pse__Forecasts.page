<apex:selectOptions value="{!periodOptions}" />
</apex:selectList>


<apex:selectList id="selectedSummaryPeriodId" disabled="{!isEditMode}" value="{!selectedSummaryPeriodId}" title="{!$Label.pse__forecasts_tool_tip_summary_period}" size="1" onchange="wait(true); refreshSummary(this.value);">
<apex:selectOptions value="{!summaryPeriodOptions}" />
</apex:selectList>

<script type="text/javascript">

 //selecting 'summary period' value based on value selected from 'entry period' value. 
//Function shall be called by ajax callback (actionFunction - id : 'refresh')
  function updateSummary(){
    var elem = document.getElementById("{!$Component.selectedSummaryPeriodId}");

    if(elem.options.length > 0){
      elem.options[0].selected = true;
      
      var selectedNode = elem.options[0].value;
      refreshSummary(selectedNode);
    }
  }
</script>


