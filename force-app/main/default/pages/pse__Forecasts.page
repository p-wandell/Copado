  <li class="{!IF(showGroups, IF(groupsSelected, 'currentTab', 'secondaryPalette'), 'hidden')}"><a onclick="wait(true); showGroups();" title="{!$Label.forecasts_label_groups_subtab}" href="">{!$Label.forecasts_label_groups}</a></li>
  <li class="{!IF(showProjects, IF(ProjectsSelected, 'currentTab', 'secondaryPalette'), 'hidden')}"><a onclick="wait(true); showProjects();" title="{!$Label.forecasts_label_projects_subtab}" href="">{!$Label.common_label_projects}</a></li>
