  text-decoration: none;
}
.ss-bootstrap a.btn,
.ss-bootstrap a.btn.ss-button {
  line-height: 18px;
  font-size: 11px;
  display: inline-block;
  padding-left: 8px;
  padding-right: 8px;
  background: #ff6600 repeat-x 0 bottom;
  color: #FFFFFF;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
}
.ss-bootstrap a:hover.btn,
.ss-bootstrap a:hover.btn.ss-button {
  color: #FFFFFF;
}
.ss-bootstrap a.btn-social {
  font-weight: normal;
  font-size: 14px;
  padding: 6px 0px;
  margin-top: 5px;
  margin-bottom: 5px;
}
.ss-bootstrap #header {
  position: relative;
  display: block;
  width: 940px;
  margin-right: auto;
  margin-left: auto;
  height: 100px;
  margin-bottom: 40px;
  border-bottom: 0px solid fuchsia;
}
.ss-bootstrap #header .logo {
  height: 80px;
  width: 150px;
  position: absolute;
  top: 10px;
}
.ss-bootstrap #header .social-navigation-header {
  position: absolute;
  top: 2px;
  right: 0px;
  height: 20px;
  width: 700px;
  text-align: right;
}
.ss-bootstrap #header .social-navigation-header .twitter-follow-button {
  padding-top: 2px;
}
.ss-bootstrap #header .navigation {
  position: absolute;
  right: 20px;
  bottom: 20px;
  display: block;
}
.ss-bootstrap #header .navigation ul.root {
  position: relative;
  background: url(menuLeft.png) repeat-x;
  height: 45px;
  max-width: 700px;
  float: left;
  padding-left: 1px;
}
.ss-bootstrap #header .navigation ul.root li {
  position: relative;
  float: left;
  line-height: 47px;
  border-right: 1px solid #4B5967;
  border-left: 1px solid #576676;
}
.ss-bootstrap #header .navigation ul.root li.menuitem-selected {
  background: url(activeMenuBG.png) repeat-x 0 3px;
}
.ss-bootstrap #header .navigation ul.root li.menuitem-selected a {
  color: #3AA9E5;
}
.ss-bootstrap #header .navigation ul.root li a {
  position: relative  !important;
  color: #585858 !important;
  text-decoration: none !important;
  font-size: 14px !important;
  display: block !important;
  padding-left: 26px !important;
  padding-right: 26px !important;
  line-height: 47px !important;
}
.ss-bootstrap #header .navigation ul.root li a:hover {
  color: #3AA9E5;
}
.ss-bootstrap #header .navigation .search {
  float: left;
  position: relative;
  background: url(menuRight.png) repeat-x right;
  height: 45px;
  max-width: 500px;
  color: #FFFFFF;
  padding-right: 26px;
  padding-left: 26px;
  line-height: 50px;
  font-size: 13px;
  width: 117px;
}
.ss-bootstrap #content {
  position: relative;
  display: block;
  width: 940px;
  margin-right: auto;
  margin-left: auto;
  border: 0px solid maroon;
  margin-bottom: 40px;
}
.ss-bootstrap #push {
  height: 30px;
}
.ss-bootstrap #toprow {
  height: 35px;
  position: relative;
  top: 1px;
  text-align: right;
  padding-right: 120px;
}
.ss-bootstrap #wrap {
  min-height: 100%;
  height: auto !important;
  height: 100%;
  /* Negative indent footer by it's height */
  margin: 0 auto -60px;
}
.ss-bootstrap #footer,
.ss-bootstrap .publishercontainer #publisherBottomBar,
.ss-bootstrap .publishercontainer .bottomBar,
.ss-bootstrap .feeditemcommentplaceholder .cxcommentplaceholderaction {
  box-sizing: content-box !important;
}
.ss-bootstrap .quickActionPanel *,
.ss-bootstrap .page-header *,
.ss-bootstrap #myModal * {
  box-sizing: border-box !important;
}
.ss-bootstrap #footer a {
  color: #FFFFFF;
  text-decoration: none;
  font-size: 11px;
  display: block;
}
.ss-bootstrap #footer a:hover {
  color: #3AA9E5;
}
