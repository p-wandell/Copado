}
.ss-bootstrap p {
  /*font-size:12px;*/
  line-height: 18px;
}
.ss-bootstrap h1 {
  margin-bottom: 10px;
  color: #3AA9E5;
  /*font-weight:200;
  font-size:18px;*/
}
.ss-bootstrap strong {
  /*font-weight:600;*/
}
.ss-bootstrap .no-case-message {
  margin-bottom: 10px;
}
.ss-bootstrap input.bs-btn-override.btn,
.ss-bootstrap button.bs-btn-override.btn {
  padding: 6px 12px;
  margin: 0;
  font-size: 14px;
  font-weight: normal;
  cursor: pointer;
  background-image: none;
  border: 1px solid transparent;
  border-radius: 4px;
}
.ss-bootstrap a,
.ss-bootstrap a.ss-link,
.ss-bootstrap #header a,
.ss-bootstrap #summits-topbar a {
