  font-size: 14px;
  margin-bottom: 5px;
}
.ss-bootstrap #footer {
  position: relative;
  display: block;
  height: 30px;
  background-color: #666666 !important;
  padding-top: 20px;
  color: #666666 !important;
  font-size: 12px;
}
.ss-bootstrap #footer-content {
  width: 940px;
  margin-right: auto;
  margin-left: auto;
}
