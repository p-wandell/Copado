  width: 160px;
  padding-right: 20px;
  margin-right: 20px;
}
.ss-bootstrap .flex-container a:active,
.ss-bootstrap .flexslider a:active {
  outline: none;
}
.ss-bootstrap .slides,
.ss-bootstrap .flex-control-nav,
.ss-bootstrap .flex-direction-nav {
  margin: 0;
  padding: 0;
  list-style: none;
}
.ss-bootstrap .flexslider {
  width: 100.0%;
  margin: 0;
  padding: 0;
}
.ss-bootstrap .flexslider .slides > li {
  display: none;
}
.ss-bootstrap .flexslider .slides img {
  max-width: 100.0%;
  display: block;
}
.ss-bootstrap .flex-pauseplay span {
  text-transform: capitalize;
}
.ss-bootstrap .slides:after {
  display: block;
  clear: both;
  visibility: hidden;
  line-height: 0;
  height: 0;
}
.ss-bootstrap html[xmlns].slides {
  display: block;
}
.ss-bootstrap * html .slides {
  height: 1.0%;
}
.ss-bootstrap .no-js .slides > li:first-child {
  display: block;
}
.ss-bootstrap .flexslider {
  position: relative;
  zoom: 1;
}
.ss-bootstrap .flexslider .slides {
  zoom: 1;
}
.ss-bootstrap .flexslider .slides > li {
  position: relative;
}
.ss-bootstrap .flex-container {
  zoom: 1;
  position: relative;
  width: 940px;
  margin-right: auto;
  margin-left: auto;
}
.ss-bootstrap .flex-direction-nav li a {
  width: 20px;
  height: 30px;
  margin: 0px 0 0;
  display: block;
  background: url(rotator-next-previous.png) no-repeat 0 0;
  position: absolute;
  top: 50.0%;
  cursor: pointer;
  text-indent: -9999px;
}
.ss-bootstrap .flex-direction-nav li .next {
  background-position: -30px 0;
  right: -40px;
}
.ss-bootstrap .flex-direction-nav li .prev {
  left: -40px;
}
.ss-bootstrap .flex-direction-nav li .disabled {
  opacity: 0.3;
  cursor: default;
}
.ss-bootstrap .flex-control-nav {
  width: 100.0%;
  position: absolute;
  bottom: -30px;
  text-align: center;
}
.ss-bootstrap .flex-control-nav li {
  margin: 0 0 0 5px;
  display: inline-block;
  zoom: 1;
}
.ss-bootstrap .flex-control-nav li:first-child {
  margin: 0;
}
.ss-bootstrap .flex-control-nav li a {
  width: 13px;
  height: 13px;
  display: block;
  background: url(theme/bg_control_nav.png) no-repeat 0 0;
  cursor: pointer;
  text-indent: -9999px;
}
.ss-bootstrap .flex-control-nav li a:hover {
  background-position: 0 -13px;
}
.ss-bootstrap .flex-control-nav li a.active {
  background-position: 0 -26px;
  cursor: default;
}
.ss-bootstrap .copyright {
  color: white !important;
  font-size: 12px !important;
  text-align: center;
  list-style: none;
  float: left;
  text-shadow: 1px 2px 1px #000;
  padding-left: 100px;
}
.ss-bootstrap #powered {
  padding-top: 30px;
  padding-left: 200px;
}
.ss-bootstrap .post-content h2,
.ss-bootstrap .title h2,
.ss-bootstrap #main .post-content .title h2,
.ss-bootstrap .page-title h1,
.ss-bootstrap #main .post h2 a {
  font-size: 24px !important;
  line-height: 36px !important;
}
.ss-bootstrap .error-image {
  background-image: url(../image/404_image@2x.png) !important;
  -webkit-background-size: 311px 133px;
  -moz-background-size: 311px 133px;
  -o-background-size: 311px 133px;
  background-size: 311px 133px;
  width: 311px;
  height: 133px;
  margin-top: 14px;
}
.ss-bootstrap #content.no-bottom {
  margin-bottom: 0px;
}
.ss-bootstrap li.tab-selected a:hover,
.ss-bootstrap .navbar-default .navbar-nav > li.tab-selected > a:hover,
.ss-bootstrap .navbar-default .navbar-nav > li.tab-selected > a {
  color: #000;
}
.ss-bootstrap #header li.tab-selected a {
  color: #000;
}
.ss-bootstrap a#create-case-home {
  padding: 10px 0px;
  font-size: 1.5em;
  margin-bottom: 15px;
}
.ss-bootstrap #content .page-header h2 {
  font-weight: normal;
}
.ss-bootstrap #content h2 {
  font-size: 30px;
}
.ss-bootstrap ul li.usericon {
  margin-left: -80px !important;
}
.ss-bootstrap div.cxfooter {
  display: none;
  visibility: hidden;
}
.ss-bootstrap .feeditemcontent label {
  display: inline;
  /* Chatter input text is not wide enough */
}
.ss-bootstrap .feeditemcontent input[type="text"] {
  font-weight: normal;
  /* Otherwise bootstrap makes this bold */
}
.ss-bootstrap .vfButtonBar h3 {
  /* Update: Mimic the h4 bootstrap tag */
  font-size: 18px;
  /* 12px; - this is the default */
  line-height: normal;
  /* 20px; - this is the default */
  font-weight: normal;
  /* bold; - this is the default */
}
@media only screen and (min-width: 768px) {
  .ss-bootstrap nav.navbar {
    display: inline-block;
    float: right;
  }
  .ss-bootstrap .navbar-nav {
    float: none;
    margin: 0px;
  }
  .ss-bootstrap div.navbar-collapse {
    padding-left: 0px;
    padding-right: 0px;
  }
}
@media only screen and (max-width: 1023px) {
  .ss-bootstrap #header .social-navigation-header,
  .ss-bootstrap #header .navigation ul.root {
    width: 100%;
  }
  .ss-bootstrap #header,
  .ss-bootstrap #content,
  .ss-bootstrap #footer-content {
    width: 90%;
  }
  .ss-bootstrap #header .navigation {
    right: 0px;
  }
  .ss-bootstrap .navbar-collapse {
    padding-right: 0px;
  }
  .ss-bootstrap #footer {
    margin-left: 0px;
    margin-right: 0px;
    padding-left: 0px;
    padding-right: 0px;
  }
  .ss-bootstrap #footer a {
    display: inline-block;
  }
}
@media only screen and (max-width: 767px) {
  .ss-bootstrap {
    /* Replace a style from bootstrap.css that adds a big margin above the top of the content */
  }
  .ss-bootstrap #header {
    height: 100%;
    margin-bottom: 0px;
    text-align: center;
  }
  .ss-bootstrap #header .logo {
    position: relative;
  }
  .ss-bootstrap #toprow {
    padding-right: 10px;
  }
  .ss-bootstrap nav.navbar {
    margin-top: 25px;
    box-sizing: border-box !important;
  }
  .ss-bootstrap nav .navbar-nav {
    margin-right: 0px;
    float: none;
  }
  .ss-bootstrap ul.copyright {
    padding-left: 0px;
    width: 100%;
  }
  .ss-bootstrap .page-header {
    margin-top: 0px;
  }
}

</apex:page>