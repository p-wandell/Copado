}
.ss-bootstrap a:hover,
.ss-bootstrap a:hover.ss-link,
.ss-bootstrap a:hover #header a:hover,
.ss-bootstrap #summits-topbar a:hover,
.ss-bootstrap .ss-bootstrap .navbar-default .navbar-nav > li > a:hover,
.ss-bootstrap .ss-bootstrap .navbar-default .navbar-nav > li > a {
