<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>Lead Assignment</fullName>
        <active>true</active>
        <ruleEntry>
            <assignedTo>jarrod.kingston@7summitsinc.com</assignedTo>
            <assignedToType>User</assignedToType>
            <formula>Lead_Source_Detail__r.Id=&quot;7011E000001c61V&quot;</formula>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>kristen.andersen@7summitsinc.com</assignedTo>
            <assignedToType>User</assignedToType>
            <criteriaItems>
                <field>Lead.Email</field>
                <operation>contains</operation>
                <value>@salesforce</value>
            </criteriaItems>
        </ruleEntry>
    </assignmentRule>
    <assignmentRule>
        <fullName>Lead Owner Reassignments</fullName>
        <active>false</active>
        <ruleEntry>
            <criteriaItems>
                <field>Lead.LeadSource</field>
                <operation>equals</operation>
                <value>Jigsaw</value>
            </criteriaItems>
        </ruleEntry>
    </assignmentRule>
</AssignmentRules>
