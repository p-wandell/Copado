<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>pse__Hierarchy_Level__c</fullName>
    <deprecated>false</deprecated>
    <description>Indicates the level within the skills hierarchy.</description>
    <externalId>false</externalId>
    <formula>IF (
    			ISBLANK ( pse__Parent_Category__c ),
    			0,
    			( LEN ( pse__Hierarchy_Key_Path__c ) + 1 ) / 16
    		) + 1</formula>
    <inlineHelpText>Indicates the level within the skills hierarchy.</inlineHelpText>
    <label>Hierarchy Level</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
