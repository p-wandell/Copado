<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ffr__NotificationType__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF(AND(ffr__TotalNumberOfErrors__c=0,ffr__TotalNumberOfAborts__c=0,ffr__TotalNumberOfExternal__c=0), 
&apos;Successful&apos;, 
IF(ffr__TotalNumberOfErrors__c = ffr__TotalNumberOfLogs__c, 
&apos;Failure&apos;, 
IF(ffr__TotalNumberOfAborts__c &gt; 0, 
&apos;Failure&apos;, 
IF( ffr__TotalNumberOfExternal__c &gt; 0, 
&apos;Failure&apos;, 
&apos;Mixed&apos; 
) 
) 
) 
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Notification Type</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
